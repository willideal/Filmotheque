{% extends 'MyAppFilmothequeBundle::layout.html.twig' %}{% block title %}Liste des acteurs{% endblock %}{% block content %}<form id="form_recherche" action="{{ path('myapp_acteur_rechercher') }}" method="post">    {{ form_widget(form) }}    <input type="submit" value="Rechercher" /></form><div class="loading"></div><div id="resultats_recherche">     {% include 'MyAppFilmothequeBundle:Acteur:liste.html.twig' with {'acteurs' : acteurs}  %}</div><p><a href="{{ path('myapp_acteur_ajouter') }}">Ajouter un acteur</a><p><script>$(".loading").hide();$("#form_recherche").submit(function(){     $(".loading").show();    var motcle = $("#acteurrecherche_motcle").val();    var DATA = 'motcle=' + motcle;    $.ajax({        type: "POST",        url: "{{ path('myapp_acteur_rechercher')}}",        data: DATA,        cache: false,        success: function(data){           $('#resultats_recherche').html(data);           $(".loading").hide();        }    });        return false;});</script>{% endblock %}